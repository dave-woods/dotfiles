{
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 13,
    "java.errors.incompleteClasspath.severity": "ignore",
    "files.associations": {
        "*.pl": "prolog",
        "*.prisma": "graphql"
    },
    "prolog.linter.run": "never",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.rendererType": "dom",
    "python.jediEnabled": false,
    "window.titleBarStyle": "custom",
    "python.languageServer": "Microsoft",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.tab.editorGroup": "left",
    "latex-workshop.view.pdf.backgroundColor": "#1E1E1E",
    "latex-workshop.viewer.pdf.internal.keyboardEvent": "force",
    "latex-workshop.message.latexlog.exclude": [
        "Command \\\\footnotesize invalid in math mode\\.",
        "Command \\\\footnotesize invalid in math mode.\\n"
    ],
    // "editor.fontFamily": "MAYconlang"
}